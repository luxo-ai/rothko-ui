{
  "background": { "value": "#141414" },
  "border": { "value": "#FFF" },
  "foreground": { "value": "#FFF" },
  "icon": {
    "background": { "value": "{foreground}" },
    "border": { "value": "{foreground}" }
  },
  "slider": {
    "handle": {
      "border": { "value": "{slider.handle.background}" },
      "background": { "value": "#2d3035" }
    },
    "range": {
      "background": { "value": "#4d63ff" }
    },
    "track": {
      "background": { "value": "rgb(255 255 255 / 8%)" }
    }
  },
  "dropdown": {
    "background": { "value": "{background}" },
    "multiselect": {
      "foreground": { "value": "#c42317" },
      "border": { "value": "{dropdown.multiselect.foreground}" },
      "background": { "value": "#000" }
    },
    "option": {
      "background::focus": { "value": "#3B3B3B" }
    }
  },
  "tabBar": {
    "border": { "value": "#FFF" }
  },
  "radio": {
    "border": { "value": "#6b6b6b" },
    "background": { "value": "#2D3035" },
    "background::focus": { "value": "{radio.background}" }
  },
  "table": {
    "background": { "value": "#080808" },
    "header": {
      "background": { "value": "{background}" }
    },
    "row": {
      "border": { "value": "#000" },
      "minimized": {
        "background": { "value": "{background}" }
      }
    }
  },
  "toast": {
    "foreground": { "value": "{foreground}" },
    "background": { "value": "{background}" },
    "life": {
      "filled": {
        "background": { "value": "#e93c2f" }
      },
      "empty": {
        "background": { "value": "transparent" }
      }
    }
  },
  "accordion": {
    "border": { "value": "{border}" },
    "background": { "value": "{background}" }
  },
  "checkbox": {
    "background": { "value": "#DEE7F5" },
    "background::focus": { "value": "#4d63ff" },
    "icon": { "background": { "value": "#fff" } }
  },
  "input": {
    "background": { "value": "rgba(188, 199, 223, 0.24)" },
    "foreground": { "value": "{foreground}" }
  },
  "skeleton": {
    "background": { "value": "#DEE7F5" },
    "foreground": { "value": "#EBF2FB" }
  },
  "switch": {
    "border": { "value": "#494949" },
    "background": { "value": "#2d3035" },
    "background::focus": { "value": "{success.default}" },
    "handle": {
      "background": { "value": "#757985" }
    }
  }
}
